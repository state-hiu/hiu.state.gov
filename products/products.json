---
layout: default
---
{
  "type": "FeatureCollection",
  "features": [{% for product in site.categories.products %}
    {% if product.lon and product.lat %}
    {
      "type": "Feature",
      "properties": {
        "title": "{{ product.title }}",
        {% if product.interactive %}
        "interactive": {{ product.interactive }},
        {% endif %}
        "date_published": "{{ product.date_published | date: '%B %d, %Y' }}",
        "region_id": "{{ product.region_id }}",
        "region_title": "{{ product.region_title }}",
        "url": "{{ site.url }}{{ product.url | remove: '.html' }}",
        "product_id": "{{ product.product_id }}",
        "product_pdf": "https://products.hiu.state.gov/{{ product.product_pdf }}",
        "product_jpg": "https://products.hiu.state.gov/{{ product.product_jpg }}",
        {% if product.product_jpg_pg2 %}
        "product_jpg_pg2": "https://products.hiu.state.gov/{{ product.product_jpg_pg2 }}",
        {% endif %}
        {% if product.product_jpg_pg3 %}
        "product_jpg_pg3": "https://products.hiu.state.gov/{{ product.product_jpg_pg3 }}",
        {% endif %}
        {% if product.product_jpg_pg4 %}
        "product_jpg_pg4": "https://products.hiu.state.gov/{{ product.product_jpg_pg4 }}",
        {% endif %}
        {% if product.product_jpg_pg5 %}
        "product_jpg_pg5": "https://products.hiu.state.gov/{{ product.product_jpg_pg5 }}",
        {% endif %}
        {% if product.product_jpg_pg6 %}
        "product_jpg_pg6": "https://products.hiu.state.gov/{{ product.product_jpg_pg6 }}",
        {% endif %}
        {% if product.product_jpg_pg7 %}
        "product_jpg_pg7": "https://products.hiu.state.gov/{{ product.product_jpg_pg7 }}",
        {% endif %}
        "thumbnail_150": "https://s3.amazonaws.com/hiu-thumbnails/products/resized/small/{{ product.thumbnail }}",
        "thumbnail_300": "https://s3.amazonaws.com/hiu-thumbnails/products/resized/middle/{{ product.thumbnail }}",
        "twitter_pic": "https://s3.amazonaws.com/hiu-thumbnails/products/twitter/{{ product.twitter_pic }}",
        "tweet": "{{ product.tweet }}",
        "marker-color": "#8cc250"
      },
      "geometry": {
        "type": "Point",
        "coordinates": [{{ product.lon }}, {{ product.lat }}]
      }
    }{% unless forloop.last %},{% endunless %}
    {% endif %}{% endfor %}
  ]
}
